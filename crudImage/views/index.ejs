<%- include('./layout/header.ejs')%>
<h1>Home Page</h1>
<div class="container">
  <div class="row my-4">
    <div class="col-lg-12">
      <% if(message) { %>
      <div class="alert alert-dismissible fade show alert-<%= message.type %>">
        <button class="btn-close" type="button" data-bs-dismiss="alert" aria-label="Close"></button>
        <strong> <%= message.message %></strong>
      </div>
      <%} %>
      <div class="table-responsive">
        <table class="table table-striped text-center">
          <thead>
            <tr class="table-dark">
              <th>ID</th>
              <th>Image</th>
              <th>Name</th>
              <th>Email</th>
              <th>Phone</th>
              <th>Action</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td>1</td>
              <td><img src="" alt="" /></td>
              <td>Marcello Luna</td>
              <td>lunmars@dunwoody.edu</td>
              <td>1234567</td>
              <td>
                <a href="" class="text-success"><i class="fas fa-edit fa-lg mx-1"></i></a>
                <a href="" class="text-danger"><i class="fas fa-trash fa-lg mx-1"></i></a>
              </td>
            </tr>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
<%- include('./layout/footer.ejs') %>
