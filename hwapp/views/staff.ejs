<!DOCTYPE html>
<html lang="en">
  <%- include("./partials/head.ejs") %>
  <body>
    <%- include("./partials/nav.ejs") %>

    <div class="container">
      <h2>Contact Us:</h2>
      <div class="row">
        <div class="input-field col-sm-3">
          <input id="fName" type="text" class="validate" name="fName" />
          <label for="fName">First Name</label>
        </div>
        <div class="input-field col-sm-3">
          <input id="lName" type="text" class="validate" name="lName" />
          <label for="lName">Last Name</label>
        </div>
        <div class="input-field col-sm-3">
          <i class="material-icons prefix">mail</i>
          <input id="email" type="text" class="validate" name="email" />
          <label for="email">Email</label>
        </div>
        <div class="input-field col-sm-3">
          <i class="material-icons prefix">phone</i>
          <input id="phone" type="text" class="validate" name="phone" />
          <label for="phone">Phone Number</label>
        </div>
      </div>
      <div class="row">
        <div class="input-field col-sm-6">
          <i class="material-icons prefix">edit_note</i>
          <input id="text" type="text" class="materialize-textarea validate" name="text" />
          <label for="text">Inquiry</label>
        </div>
        <div class="col-sm-6">
          <button class="btn btn-large">Submit</button>
        </div>
      </div>
    </div>
  </body>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
</html>
