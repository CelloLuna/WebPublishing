//standard form for what we have been working on
var MongoClient = require('mongodb').MongoClient;
var url = "mongodb://127.0.0.1:27017";

MongoClient.connect(url, function(err, db){
    if (err) throw err;
    var dbo = db.db("");
    dbo.collection("")  (function(err, result){
        if (err) throw err;
        console.log();
        db.close();
    });
});